{
    "name": "soap/flysystem-onedrive",
    "description": "This is my package flysystem-onedrive",
    "keywords": [
        "soap",
        "flysystem-onedrive"
    ],
    "homepage": "https://github.com/soap/flysystem-onedrive",
    "license": "MIT",
    "authors": [
        {
            "name": "Prasit Gebsaap",
            "email": "prasit.gebsaap@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "league/flysystem": "^1.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Soap\\FlysystemOnedrive\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Soap\\FlysystemOnedrive\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
